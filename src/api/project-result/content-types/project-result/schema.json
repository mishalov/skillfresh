{
  "kind": "collectionType",
  "collectionName": "project_results",
  "info": {
    "singularName": "project-result",
    "pluralName": "project-results",
    "displayName": "ProjectResult",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "deadline": {
      "type": "date",
      "required": false
    },
    "reviewResult": {
      "type": "blocks"
    },
    "revealed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "project_results"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "projectResults"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "In Progress",
        "In Review",
        "Completed"
      ],
      "required": true,
      "default": "In Progress"
    }
  }
}
